{
  "name": "entregable",
  "version": "1.0.0",
  "description": "",
  "main": "./src/app.js",
  "scripts": {
    "start": "node . --port 8080",
    "start:cluster": "node . --port 8080 --mode cluster",
    "dev": "nodemon --legacy-watch . --port 8080",
    "dev:cluster": "nodemon --legacy-watch . --port 8080 --mode cluster",
    "forever:fork": "forever start --watch . --port 8080",
    "forever:cluster": "forever start --watch . --port 8080 --mode cluster",
    "pm2:fork": "pm2 start worker.config.js",
    "pm2:cluster": "pm2 start cluster.config.js",
    "test:artillery": "artillery quick --count=50 -n 20 http://node:8080/info > artillery_without_console_result.txt",
    "test:artillery-console": "artillery quick --count=50 -n 20 http://node:8080/info?console=1 > artillery_with_console_result.txt",
    "test:autocannon": "node ./tests/info.spec.js",
    "test:0x": "0x .",
    "test:chrome-profiler": "node --inspect-brk=0.0.0.0 .",
    "test:jest": "jest --setupFiles dotenv/config",
    "test:mocha": "mocha './tests/unit/**/*.test.js'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "nodemonConfig": {
    "ignore": [
      "./scripts",
      "./src/public/*",
      "./.db/*",
      "./tests"
    ]
  },
  "dependencies": {
    "@vscode/sqlite3": "^5.0.8",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "connect-mongo": "^4.6.0",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.2",
    "firebase-admin": "^10.0.2",
    "forever": "^4.0.3",
    "got": "^11.8.5",
    "knex": "^1.0.4",
    "mongoose": "^6.2.9",
    "multer": "^1.4.4",
    "mysql": "^2.18.1",
    "nodemailer": "^6.7.5",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "pm2": "^5.2.0",
    "pug": "^3.0.2",
    "redis": "^4.0.4",
    "socket.io": "^4.4.1",
    "twilio": "^3.77.0",
    "winston": "^3.7.2",
    "yargs": "^17.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "0x": "^5.2.1",
    "artillery": "^2.0.0-14",
    "autocannon": "^7.8.1",
    "chai": "^4.3.6",
    "faker": "^5.5.3",
    "jest": "^27.5.1",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.3"
  }
}
